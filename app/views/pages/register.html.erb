<h1>Create an Account</h1>

<%= bootstrap_form_with model: @pages, url: welcome_path, method: :post, local: true do |f| %>

  <%= f.text_field :fname %>
  <%= f.text_field :lname %>
  <%= f.text_field :email %>
  <%= f.text_field :pass %>
  <%= f.text_field :repass %>
  <%= f.text_field :pnum %>
  <%= f.submit %>

<% end %>
